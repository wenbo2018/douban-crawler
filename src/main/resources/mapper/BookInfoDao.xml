<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.com.github.wenbo2018.douban.crawler.dao.BookInfoDao">

    <resultMap id="BaseResultMap" type="com.github.com.github.wenbo2018.douban.crawler.entity.BookInfo">
        <result column="Id" property="id" jdbcType="INTEGER" />
        <result column="BookName" property="bookName" jdbcType="VARCHAR" />
        <result column="Author" property="author" jdbcType="VARCHAR" />
        <result column="Press" property="press" jdbcType="VARCHAR" />
        <result column="PressTime" property="pressTime" jdbcType="VARCHAR" />
        <result column="Price" property="price" jdbcType="DOUBLE" />
        <result column="Star" property="star" jdbcType="DOUBLE" />
        <result column="EvaluateCount" property="evaluateCount" jdbcType="INTEGER" />
        <result column="BookType" property="BookType" jdbcType="INTEGER" />
        <result column="AddTime" property="addTime" jdbcType="TIMESTAMP" />
        <result column="UpdateTime" property="updateTime" jdbcType="TIMESTAMP" />
    </resultMap>


    <insert id="addBookInfo" parameterType="com.github.com.github.wenbo2018.douban.crawler.entity.BookInfo" >
        insert into User (
        BookName,
        Author,
        Press,
        PressTime,
        Price,
        Star,
        EvaluateCount,
        BookType,
        AddTime,
        UpdateTime
        )
        values (
        #{bookName},
        #{author},
        #{press},
        #{pressTime},
        #{price},
        #{star},
        #{evaluateCount},
        #{BookType},
        CURRENT_TIMESTAMP,
        CURRENT_TIMESTAMP
        )
    </insert>

</mapper>